        class="demo-table-expand"
      >
        <el-form-item style="width:20%;" label="楼栋:" prop="buildId">
          <el-select v-model.trim="formInline.buildId" placeholder="全部" @change="getFloorInfo">
            <el-option
              v-for="item in buildList"
              :key="item.value"
